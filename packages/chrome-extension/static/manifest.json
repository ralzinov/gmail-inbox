{
  "manifest_version": 2,
  "name": "Gmail inbox preview - extension for Chrome",
  "description": "This extension watches Gmail and allows to preview and manage inbox",
  "version": "0.0.1",

  "browser_action": {
    "default_icon": {
      "16": "icons/16.png",
      "24": "icons/24.png",
      "32": "icons/32.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "permissions": [
    "background",
    "identity",
    "alarms",
    "notifications",
    "cookies",
    "https://mail.google.com/"
  ],
  "oauth2": {
    "client_id": "466668520976-u8a8p0lojgk60gs75rmodp0cdpvlg00a.apps.googleusercontent.com",
    "scopes": [
      "https://mail.google.com/",
      "https://www.googleapis.com/auth/gmail.readonly",
      "https://www.googleapis.com/auth/gmail.metadata",
      "https://www.googleapis.com/auth/gmail.modify"
    ]
  },
  "key": "",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://apis.google.com/; object-src 'self'"
}
